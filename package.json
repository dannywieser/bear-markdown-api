{
  "name": "markdownmemory",
  "version": "0.0.1",
  "keywords": [
    "markdown"
  ],
  "description": "A static site generator for Markdown focused on reliving the memories captured in your writing.",
  "homepage": "https://github.com/dannywieser/markdownmemory#readme",
  "bugs": {
    "url": "https://github.com/dannywieser/markdownmemory/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dannywieser/markdownmemory.git"
  },
  "license": "MIT",
  "author": "Danny Wieser",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc && vite build ./src/web",
    "build-server": "tsc",
    "build-web": "vite build ./src/web",
    "start": "node ./dist/server/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' ./src/server/server.ts",
    "web": "vite ./src/web",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@eslint/js": "9.33.0",
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/node": "24.3.0",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react": "5.0.0",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-perfectionist": "4.15.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "globals": "16.3.0",
    "jest": "30.0.5",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "supertest": "7.1.4",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.2",
    "typescript-eslint": "8.40.0",
    "vite": "7.1.2"
  },
  "dependencies": {
    "date-fns": "4.1.0",
    "express": "5.1.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "sqlite": "5.1.1",
    "sqlite3": "5.1.7"
  }
}
